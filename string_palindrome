import java.util.*;
public class Main{
    public static void main(String[]args){
        String s1="madam";
        boolean flag=false;
        for(int i=0;i<s1.length()/2;i++){
            if(s1.charAt(i)!=s1.charAt(s1.length()-i-1)){
                flag=true;
                System.out.print("not a palindrome");
            }
            
            }
            System.out.print("palindrome");
