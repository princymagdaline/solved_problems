import java.util.*;
public class Main{
    public static void main(String[] args) {
        String s="i am a girl";
        String[] str=s.split(" ");
        for(int i=0;i<str.length/2;i++)
        { 
          String temp=str[i];
          str[i]=str[(str.length-i)-1];
          str[(str.length-i)-1]=temp;
        }
        for(int i=0;i<str.length;i++)
        { 
        System.out.print(str[i]);       
        } 
    }
}
